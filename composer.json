{
  "name": "bzga/bzga-beratungsstellensuche",
  "type": "typo3-cms-extension",
  "description": "Beratungsstellensuche der BZgA",
  "version": "6.2.0",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Sebastian Schreiber",
      "email": "ssch@hauptweg-nebenwege.de",
      "role": "Developer"
    },
    {
      "name": "Sebastian Hebler",
      "email": "sh@hauptweg-nebenwege.de",
      "role": "Projectmanager"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bzga\\BzgaBeratungsstellensuche\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bzga\\BzgaBeratungsstellensuche\\Tests\\": "Tests/",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
      "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
    }
  },
  "require": {
    "typo3/cms-core": "^6.2 || ^7.6 || ^8.7 || ^9.5",
    "php": ">=5.5.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7 || ^5.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "nimut/testing-framework": "^1.0",
    "symfony/serializer": "^3.0",
    "ezyang/htmlpurifier": "^4.8",
    "egeloen/google-map": "^2.0",
    "willdurand/geocoder": "^3.3",
    "sjbr/static-info-tables": "^6.0"
  },
  "suggest": {
    "typo3-ter/static-info-tables": "We need it to map Languages and Country-Zones",
    "typo3-ter/static-info-tables-de": "We need it to map Languages and Country-Zones",
    "typo3-ter/scheduler": "We need it to add XML-Importer task"
  },
  "replace": {
    "bzga_beratungsstellensuche": "self.version",
    "typo3-ter/bzga-beratungsstellensuche": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "preferred-install": {
      "typo3/cms": "source",
      "typo3/cms-core": "source",
      "*": "dist"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/bzga_beratungsstellensuche ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/bzga_beratungsstellensuche"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.2.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
